<div class="basic-form section">
  <div class="row">
    <div class="col s12 m12 l6 offset-l3">
      <div class="card-panel">
        <h4>Iniciar sesi칩n</h4>
        <div class="row">
          <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "col s12" }) do |f| %>
            <div class="row">
              <div class="input-field col s12">
                <%= f.label :email, "Correo electr칩nico" %><br />
                <%= f.email_field :email, autofocus: true %>
              </div>
            </div>
            <div class="row">
              <div class="input-field col s12">
                <%= f.label :password, "Contrase침a" %><br />
                <%= f.password_field :password, autocomplete: "off" %>
              </div>
            </div>
            <% if devise_mapping.rememberable? -%>
              <div class="row">
                <div class="input-field col s12">
                  <%= f.check_box :remember_me %>
                  <%= f.label :remember_me, "Recordar" %>
                </div>
              </div>
            <% end -%>
            <div class="row">
              <div class="actions col s12">
                <%= f.submit "Iniciar sesi칩n", class: "btn cyan waves-effect waves-light right" %>
              </div>
            </div>
          <% end %>
          <%= render "devise/shared/links" %>
        </div>
      </div>
    </div>
  </div>
</div>
